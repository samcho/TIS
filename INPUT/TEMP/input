set dynamics underdamped;
set temp TEMP;
set run SEED;
set restart off;
set istep_restart 0.0;
set nstep NSTEP;
set nup 10000;
set zeta ZETA;
;
load bonds DATA/ID_bonds.dat;
load angles DATA/ID_angles.dat;
load torsions DATA/ID_torsions.dat;
load stacks DATA/ID_stacks.dat;
load vdw DATA/ID_nb.dat;
load elec DATA/ID_elec.dat;
load init DATA/ID_init.xyz;
load nat DATA/ID_nat.xyz;
;
set ufname OUTPUT/ID.TEMP.SEED.out;
set cfname OUTPUT/ID.TEMP.SEED.coords.out;
set unccfname OUTPUT/ID.TEMP.SEED.coords_uncorrected.out;
set uncbinfname OUTPUT/ID.TEMP.SEED.traj_uncorrected.bin;
set vfname OUTPUT/ID.TEMP.SEED.velocs.out;
set binfname OUTPUT/ID.TEMP.SEED.traj.bin;
set iccnfigfname OUTPUT/ID.TEMP.SEED.iccnfig.xyz;
set rgenfname OUTPUT/ID.TEMP.SEED.rgen.dat;
run;
