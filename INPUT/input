set dynamics underdamped;
set temp 0.60;
set run 1;
set restart off;
set istep_restart 0.0;
set nstep 10000;
set nup 10000;
set zeta 1e-3;
set cutofftype neighborlist;
set nnlup 50;
set boxl 75.0; lcell = boxl / ncell = ~9.09
;
load bonds DATA/1ehz_bonds.dat;
load angles DATA/1ehz_angles.dat;
load torsions DATA/1ehz_torsions.dat;
load stacks DATA/1ehz_stacks.dat;
load vdw DATA/1ehz_nb.dat;
load elec DATA/1ehz_elec.dat;
load init DATA/1ehz_init.xyz;
;
set ufname OUTPUT/1ehz.0.60.1.out;
set cfname OUTPUT/1ehz.0.60.1.coords.out;
set unccfname OUTPUT/1ehz.0.60.1.coords_uncorrected.out;
set uncbinfname OUTPUT/1ehz.0.60.1.traj_uncorrected.bin;
set vfname OUTPUT/1ehz.0.60.1.velocs.out;
set binfname OUTPUT/1ehz.0.60.1.traj.bin;
set iccnfigfname OUTPUT/1ehz.0.60.1.iccnfig.xyz;
set rgenfname OUTPUT/1ehz.0.60.1.rgen.dat;
run;
